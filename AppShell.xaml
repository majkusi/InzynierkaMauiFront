<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="InzynierkaMauiFront.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:InzynierkaMauiFront"
    xmlns:pages="clr-namespace:InzynierkaMauiFront.Pages"
    Title="InzynierkaMauiFront"
    >


    <ShellContent ContentTemplate="{DataTemplate pages:LoginPage}" />

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <Tab Title="Menu glowne">
            <ShellContent ContentTemplate="{DataTemplate pages:MainMenuPage}" />
        </Tab>
    </FlyoutItem>

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <Tab Title="Plan zajec">
            <ShellContent ContentTemplate="{DataTemplate pages:TodaysCoursesPage}" />
        </Tab>
    </FlyoutItem>

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <Tab Title="Listy obecnosci">
            <ShellContent ContentTemplate="{DataTemplate pages:AttendanceRaportPage}" />
        </Tab>
    </FlyoutItem>



</Shell>